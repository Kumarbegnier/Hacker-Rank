def nextMove(n, r, c, grid):
    # Find the position of the princess
    for i in range(n):
        for j in range(n):
            if grid[i][j] == 'p':
                # Determine relative position of princess
                if i < r:
                    return "UP"
                elif i > r:
                    return "DOWN"
                elif j < c:
                    return "LEFT"
                elif j > c:
                    return "RIGHT"

n = int(input())
r, c = map(int, input().strip().split())
grid = []
for i in range(n):
    grid.append(input().strip())

print(nextMove(n, r, c, grid))
